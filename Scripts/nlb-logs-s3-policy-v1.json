{
    "Version": "2012-10-17",
    "Id": "AWSLogDeliveryWrite20150319",
    "Statement": [
        {
            "Sid": "AWSLogDeliveryWriteALB",
            "Effect": "Allow",
            "Principal": {
                "Service": "delivery.logs.amazonaws.com"
            },
            "Action": "s3:PutObject",
            "Resource": "arn:aws:s3:::awsb51logs/flowlogs/AWSLogs/721834156908/*",
            "Condition": {
                "StringEquals": {
                    "aws:SourceAccount": "721834156908",
                    "s3:x-amz-acl": "bucket-owner-full-control"
                },
                "ArnLike": {
                    "aws:SourceArn": "arn:aws:logs:us-east-1:721834156908:*"
                }
            }
        },
        {
            "Sid": "AWSLogDeliveryWrite",
            "Effect": "Allow",
            "Principal": {
                "Service": "delivery.logs.amazonaws.com"
                },
                "Action": "s3:PutObject",
                "Resource": "arn:aws:s3:::awsb51logs/AWSLogs/*",
                "Condition": {
                    "StringEquals": {
                        "s3:x-amz-acl": "bucket-owner-full-control",
                        "aws:SourceAccount": [
                            "721834156908"
                        ]
                    },
                    "ArnLike": {
                        "aws:SourceArn": [
                            "arn:aws:logs:us-east-1:721834156908:*"
                        ]
                    }
                }
        },
        {
            "Sid": "AWSLogDeliveryAclCheck",
            "Effect": "Allow",
            "Principal": {
                "Service": "delivery.logs.amazonaws.com"
            },
            "Action": "s3:GetBucketAcl",
            "Resource": "arn:aws:s3:::awsb51logs",
            "Condition": {
                "StringEquals": {
                    "aws:SourceAccount": "721834156908"
                },
                "ArnLike": {
                    "aws:SourceArn": "arn:aws:logs:us-east-1:721834156908:*"
                }
            }
        }
    ]
}